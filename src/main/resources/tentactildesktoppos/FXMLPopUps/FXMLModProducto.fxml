<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tentactildesktoppos.FXMLPopUps.FXMLModProductoController">
    <stylesheets>
        <URL value="@/tentactildesktoppos/MainCSS.css" />
    </stylesheets>
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <Label styleClass="lb_titulo" stylesheets="@../MainCSS.css" text="Modificar Producto" BorderPane.alignment="CENTER" />
         </top>
         <center>
            <VBox BorderPane.alignment="CENTER">
               <children>
                  <HBox stylesheets="@../MainCSS.css">
                     <children>
                        <Label styleClass="lb_normal" stylesheets="@../MainCSS.css" text="Codigo">
                           <HBox.margin>
                              <Insets right="10.0" top="5.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="tf_codigo_modpro" onAction="#onAction_tf_codigo_modpro" styleClass="tf_normal">
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox stylesheets="@../MainCSS.css">
                     <children>
                        <Label styleClass="lb_normal" stylesheets="@../MainCSS.css" text="Descripcion">
                           <HBox.margin>
                              <Insets right="15.0" top="5.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="tf_descri_modpro" onAction="#onAction_tf_descri_modpro" styleClass="tf_normal" />
                     </children>
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox stylesheets="@../MainCSS.css">
                     <children>
                        <Label styleClass="lb_normal" stylesheets="@../MainCSS.css" text="Stock">
                           <HBox.margin>
                              <Insets right="15.0" top="5.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="tf_stock_modpro" onAction="#onAction_tf_stock_modpro" styleClass="tf_normal" />
                     </children>
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="35.0" stylesheets="@../MainCSS.css">
                     <children>
                        <Label styleClass="lb_normal" stylesheets="@../MainCSS.css" text="Costo">
                           <HBox.margin>
                              <Insets right="15.0" top="5.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="tf_costo_modpro" onAction="#onAction_tf_costo_modpro" styleClass="tf_normal">
                           <HBox.margin>
                              <Insets right="40.0" />
                           </HBox.margin>
                        </TextField>
                        <Label layoutX="10.0" layoutY="10.0" styleClass="lb_normal" stylesheets="@../MainCSS.css" text="Iva">
                           <HBox.margin>
                              <Insets right="15.0" top="5.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="tf_iva_modpro" layoutX="41.0" layoutY="10.0" onAction="#onActionl_tf_iva_modpro" styleClass="tf_normal" />
                     </children>
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="110.0" stylesheets="@../MainCSS.css">
                     <children>
                        <Label styleClass="lb_normal" stylesheets="@../MainCSS.css" text="Precio de Venta Por mayor">
                           <HBox.margin>
                              <Insets right="15.0" top="5.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="tf_pvmayor_modpro" onAction="#onAction_tf_pvmayor_modpro" styleClass="tf_normal" />
                     </children>
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="35.0" stylesheets="@../MainCSS.css">
                     <children>
                        <Label styleClass="lb_normal" stylesheets="@../MainCSS.css" text="Precio de venta Contado">
                           <HBox.margin>
                              <Insets right="15.0" top="5.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="tf_pvcontado_modpro" onAction="#onAction_tf_pvcontado_modpro" styleClass="tf_normal" />
                     </children>
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="60.0" stylesheets="@../MainCSS.css">
                     <children>
                        <Label styleClass="lb_normal" stylesheets="@../MainCSS.css" text="Precio de Venta Credito">
                           <HBox.margin>
                              <Insets right="15.0" top="5.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="tf_pvcredito_modpro" onAction="#onAction_tf_pvcredito_modpro" styleClass="tf_normal" />
                     </children>
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </HBox>
               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
            </VBox>
         </center>
         <right>
            <VBox alignment="TOP_CENTER" BorderPane.alignment="CENTER">
               <children>
                  <Label styleClass="lb_normal" stylesheets="@../MainCSS.css" text="Datos Actuales del Producto">
                     <VBox.margin>
                        <Insets bottom="15.0" />
                     </VBox.margin>
                  </Label>
                  <TextArea fx:id="ta_datos_act_modpro" prefWidth="350.0" />
               </children>
               <BorderPane.margin>
                  <Insets bottom="5.0" left="30.0" right="10.0" top="10.0" />
               </BorderPane.margin>
            </VBox>
         </right>
         <bottom>
            <HBox alignment="TOP_CENTER" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#onClick_b_cancel_modpro" prefWidth="130.0" styleClass="botonN" stylesheets="@../MainCSS.css" text="Cancelar">
                     <HBox.margin>
                        <Insets right="30.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onMouseClicked="#onClick_b_reset_modpro" prefWidth="100.0" styleClass="botonN" stylesheets="@../MainCSS.css" text="Reset">
                     <HBox.margin>
                        <Insets left="30.0" right="30.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onMouseClicked="#onClick_b_modifcar_modpro" styleClass="botonD" stylesheets="@../MainCSS.css" text="Hacer Modificacion">
                     <HBox.margin>
                        <Insets left="30.0" right="30.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onMouseClicked="#onClick_b_eliminar_modpro" styleClass="botonDE" stylesheets="@../MainCSS.css" text="Eliminar">
                     <HBox.margin>
                        <Insets left="30.0" right="30.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <BorderPane.margin>
                  <Insets bottom="20.0" top="30.0" />
               </BorderPane.margin>
            </HBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
